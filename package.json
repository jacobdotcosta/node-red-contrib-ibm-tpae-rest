{
	"name": "node-red-contrib-ibm-tpae-rest",
	"version": "0.0.1",
	"description": "A Node-RED node to interact with IBM TPAE (IBM Maximo and IBM Control Desk) REST API.",
	"main": "index.js",
	"node-red": {
		"nodes": {
			"mbo": "mbo.js",
			"tpaeServer": "tpaeServer.js"
		}
	},
	"scripts": {
		"test": "nyc mocha ./test/*.js",
		"test-it": "nyc mocha ./it/**/*.js",
		"test-jest": "jest --verbose ./test --coverage",
		"test-jest-it": "jest --verbose ./it --coverage",
		"int-test": "mocha --require babel-core/register --recursive test",
		"test-workorder": "node test/workorder_spec.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/trikorasolns/node-red-contrib-ibm-tpae-rest.git"
	},
	"keywords": [
		"ibm-tpae",
		"ibm-maximo",
		"ibm-control-desk",
		"rest"
	],
	"author": "Trikora Solutions SL",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/trikorasolns/node-red-contrib-ibm-tpae-rest/issues"
	},
	"homepage": "https://github.com/trikorasolns/node-red-contrib-ibm-tpae-rest#readme",
	"devDependencies": {
		"jest": "^28.1.3",
		"mocha": "^10.0.0",
		"mustache": "^4.2.0",
		"node-red": "^2.2.2",
		"node-red-node-test-helper": "^0.3.0",
		"nyc": "^15.1.0",
		"typescript": "^4.7.4",
		"yargs": "^17.5.1"
	}
}
